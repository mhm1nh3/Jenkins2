#!/bin/bash
awscli () { 
   apt update -y
   apt install aws cli
if [ !? -ne 0 ]
echo "its not installed"
fi
}

netcat () { 
   apt update -y
   apt install netcat
if [ !? -ne 0 ]
echo "netcat not installed"
fi
}
tsh () { 
   apt update -y
   apt install netcat
if [ !? -ne 0 ]
echo "tsh not installed"
fi
}
